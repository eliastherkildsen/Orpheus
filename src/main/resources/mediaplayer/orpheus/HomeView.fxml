<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.media.*?>
<?import javafx.scene.text.*?>

<BorderPane fx:id="homePane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="20" minWidth="20" prefHeight="500" prefWidth="800" styleClass="main" stylesheets="@../../css/styles.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mediaplayer.orpheus.Controler.HomeViewController">
   <left>
      <VBox alignment="TOP_CENTER" prefHeight="419.0" prefWidth="100.0" styleClass="secondary" stylesheets="@../../css/styles.css" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnHome" mnemonicParsing="false" onAction="#switchToHomeView" prefHeight="38.0" prefWidth="139.0">
            <graphic>
               <ImageView fitHeight="42.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@../../css/images/home.png" />
                  </image>
               </ImageView>
            </graphic>
            </Button>
            <Label text="Home" />

            <Button fx:id="btnSearch" mnemonicParsing="false" onAction="#switchToSearchView" prefHeight="38.0" prefWidth="139.0" styleClass="button" stylesheets="@../../css/styles.css">
               <graphic>
                  <ImageView fitHeight="42.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../css/images/search.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Label fx:id="labSearch" alignment="CENTER" contentDisplay="CENTER" text="Search" textFill="WHITE" />

            <Button fx:id="btnPlaylist" mnemonicParsing="false" onAction="#switchToPlaylistView" prefHeight="38.0" prefWidth="139.0" styleClass="button" stylesheets="@../../css/styles.css">
               <graphic>
                  <ImageView fitHeight="42.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../css/images/list-video.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Label fx:id="labPlaylist" text="Playlist" textFill="WHITE" />

            <Button fx:id="btnImport" mnemonicParsing="false" onAction="#onActionbtnImportClick" prefHeight="38.0" prefWidth="139.0" styleClass="button" stylesheets="@../../css/styles.css">
               <graphic>
                  <ImageView fitHeight="42.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../css/images/file-up.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Label fx:id="labImport" text="Import" textFill="WHITE" />
            <ImageView fx:id="imgThumbnail" fitHeight="92.0" fitWidth="92.0" pickOnBounds="true">
               <image>
                  <Image url="@../../css/images/download.png" />
               </image>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </ImageView>
            <VBox alignment="CENTER_LEFT" prefHeight="67.0" prefWidth="100.0">
               <children>
                  <Label fx:id="labMediaName" text="Media name" />
                  <Label fx:id="labArtistName" text="Artist name" />
               </children>
            </VBox>

         </children></VBox>
   </left>
   <bottom>
      <HBox alignment="CENTER" prefHeight="86.0" prefWidth="676.0" styleClass="secondary" stylesheets="@../../css/styles.css" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="92.0" prefWidth="207.0" />
            <VBox alignment="CENTER" prefHeight="92.0" prefWidth="567.0">
               <children>
                  <HBox fx:id="hboxButtons" alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="btnShuffle" mnemonicParsing="false" styleClass="button" stylesheets="@../../css/styles.css">
                           <graphic>
                              <ImageView fitHeight="42.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../css/images/shuffle.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>

                        <Button fx:id="brnSkipBackwards" mnemonicParsing="false" onAction="#onBtnSkipBackwardsClick" styleClass="button" stylesheets="@../../css/styles.css">
                           <graphic>
                              <ImageView fitHeight="42.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../css/images/skip-back.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>

                        <Button fx:id="btnPrevious" mnemonicParsing="false" onAction="#onBtnPreviousClick" styleClass="button" stylesheets="@../../css/styles.css">
                           <graphic>
                              <ImageView fitHeight="42.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../css/images/step-back.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>

                        <Button fx:id="btnPlayPause" mnemonicParsing="false" onAction="#onBtnPlayPauseClick" styleClass="button" stylesheets="@../../css/styles.css">
                           <graphic>
                              <ImageView fx:id="btnPlayPauseIcon" fitHeight="42.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../css/images/play-circle.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>

                        <Button fx:id="btnNext" mnemonicParsing="false" onAction="#onBtnNextClick" styleClass="button" stylesheets="@../../css/styles.css">
                           <graphic>
                              <ImageView fitHeight="42.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../css/images/step-forward.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>

                        <Button fx:id="btnSkipForwards" mnemonicParsing="false" onAction="#onBtnSkipForwardsClick" styleClass="button" stylesheets="@../../css/styles.css">
                           <graphic>
                              <ImageView fitHeight="42.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../css/images/skip-forward.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                     </children>
                     <padding>
                        <Insets right="140.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="labCurrentTime" text="00:00:00" />
                        <Slider id="custom-slider .track" fx:id="sliderProgres" blockIncrement="1.0" max="100" min="0" onMouseClicked="#onSliderProgresMouseClick" onMousePressed="#onSliderProgresMousePressed" onMouseReleased="#onSliderProgresMouseReleased" prefHeight="14.0" prefWidth="247.0" styleClass="slider" stylesheets="@../../css/progressBar.css" />
                        <Label fx:id="labMediaLength" text="00:00:00" />
                        <Button fx:id="btnVolumeMute" mnemonicParsing="false" onAction="#onBtnVolumeMuteClick" styleClass="button" stylesheets="@../../css/styles.css">
                           <graphic>
                              <ImageView fx:id="btnMuteIcon" fitHeight="27.0" fitWidth="58.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../css/images/volume-1.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                           <HBox.margin>
                              <Insets left="15.0" />
                           </HBox.margin>
                        </Button>
                        <Slider fx:id="sliderVolume" blockIncrement="5.0" onMouseClicked="#onSliderVolumeMouseClick" onMouseReleased="#onSliderVolumeMouseReleased" styleClass="slider" stylesheets="@../../css/styles.css" value="15.0" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children></HBox>
   </bottom>
   <center>
      <AnchorPane minHeight="100.0" minWidth="100.0" prefHeight="200.0" prefWidth="200.0">
         <children>
            <MediaView fx:id="mediaViewDisplay" fitHeight="410.0" fitWidth="700.0">
               <viewport>
                  <Rectangle2D />
               </viewport>
            </MediaView>
            <Label fx:id="mediaViewTitle" alignment="TOP_LEFT" layoutX="1" layoutY="1" style="-fx-text-fill: white;" text="ERROR: No Title Found?" visible="false">
               <font>
                  <Font name="Courier New Bold" size="14.0" />
               </font></Label>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
